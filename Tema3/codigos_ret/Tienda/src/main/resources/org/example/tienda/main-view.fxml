<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.FlowPane?>
<FlowPane alignment="CENTER" xmlns:fx="http://javafx.com/fxml"
          fx:controller="org.example.tienda.controller.MainController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Button text="Agregar" fx:id="botonAgregar"/>
    <Button text="Borrar" fx:id="botonBorrar"/>
    <Button text="Actualizar" fx:id="botonActualizar"/>
    <Button text="Consultar" fx:id="botonConsultar"/>
</FlowPane>
